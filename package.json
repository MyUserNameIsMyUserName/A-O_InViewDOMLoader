	"dependencies": {
		"dotenv": "^9.0.2",
		"express": "^4.17.1",
		"is-online": "^9.0.1",
		"jshint": "^2.13.0",
		"lodash": "^4.17.20",
		"nodemon": "^2.0.12",
		"open": "^8.2.1",
		"page-fps": "^1.0.2",
		"systeminformation": "^5.7.8",
		"username": "^5.1.0",
		"web-vitals": "^2.1.0"
	},
